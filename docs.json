{
  "$schema": "https://mintlify.com/docs.json",
  "theme": "mint",
  "name": "SaveGate Documentation",
  "url": "https://docs.savegate.ai",
  "colors": {
    "primary": "#0D9373",
    "light": "#10B981",
    "dark": "#047857"
  },
  "favicon": "/favicon.svg",
  "navigation": {
    "tabs": [
      {
        "tab": "Documentation",
        "groups": [
          {
            "group": "Getting Started",
            "pages": [
              "index",
              "quickstart",
              "authentication"
            ]
          },
          {
            "group": "Core Concepts",
            "pages": [
              "concepts/models",
              "concepts/pricing",
              "concepts/rate-limits"
            ]
          },
          {
            "group": "SDK Integration",
            "pages": [
              "sdk/litellm",
              "sdk/openai",
              "sdk/anthropic",
              "sdk/python",
              "sdk/nodejs"
            ]
          },
          {
            "group": "Guides",
            "pages": [
              "guides/migration",
              "guides/streaming",
              "guides/error-handling",
              "guides/best-practices"
            ]
          }
        ]
      },
      {
        "tab": "API Reference",
        "groups": [
          {
            "group": "Endpoints",
            "pages": [
              "api-reference/chat-completions",
              "api-reference/completions",
              "api-reference/embeddings",
              "api-reference/models"
            ]
          },
          {
            "group": "Examples",
            "pages": [
              "examples/basic-chat",
              "examples/streaming",
              "examples/function-calling",
              "examples/multi-model"
            ]
          }
        ]
      }
    ],
    "global": {
      "anchors": [
        {
          "anchor": "Website",
          "href": "https://savegate.ai",
          "icon": "globe"
        }
      ]
    }
  },
  "logo": {
    "light": "/logo/light.svg",
    "dark": "/logo/dark.svg"
  },
  "navbar": {
    "links": [
      {
        "label": "Contact",
        "href": "mailto:contact@savegate.ai"
      }
    ],
    "primary": {
      "type": "button",
      "label": "Dashboard",
      "href": "https://savegate.ai/signin"
    }
  },
  "api": {
    "playground": {
      "mode": "simple"
    }
  },
  "footer": {
    "socials": {
      "github": "https://github.com/savegate"
    }
  }
}
